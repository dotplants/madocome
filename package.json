{
  "name": "@nzws/live-viewer",
  "version": "0.1.0",
  "author": "nzws <git@nzws.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore \"**/*.js\"",
    "lint:styled": "stylelint --ignore-path .gitignore \"**/*.js\"",
    "lint:prettier": "pretty-quick --check",
    "lint": "npm-run-all lint:* -s",
    "format:js": "eslint --fix --ignore-path .gitignore \"**/*.js\"",
    "format:prettier": "pretty-quick",
    "format": "npm-run-all format:* -s",
    "build": "parcel build src/index.html --no-source-maps",
    "dev": "parcel src/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@yuzulabo/eslint-config": "~1",
    "@yuzulabo/prettier-config": "~1",
    "babel-eslint": "^10.0.3",
    "eslint": "~6",
    "eslint-plugin-react": "^7.17.0",
    "npm-run-all": "~4",
    "parcel-bundler": "~1",
    "prettier": "~1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0"
  },
  "dependencies": {
    "polished": "^3.4.2",
    "preact": "^10.1.1",
    "prop-types": "^15.7.2",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "prettier": "@yuzulabo/prettier-config"
}
